{
  "rules": {
    "no-for-in": true,
    "only-arrow-functions": true,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "no-duplicate-super": true,
    "no-duplicate-imports": true,
    "no-duplicate-variable": true,
    "no-duplicate-switch-case": true,
    "no-empty": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-misused-new": true,
    "no-string-literal": true,
    "no-string-throw": true,
    "no-tautology-expression": true,
    "no-unbound-method": [
      true,
      "ignore-static"
    ],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ],
    "no-var-keyword": true,
    "prefer-conditional-expression": [
      true,
      "check-else-if"
    ],
    "restrict-plus-operands": true,
    "static-this": true,
    "switch-default": true,
    "triple-equals": [
      true,
      "allow-undefined-check",
      "allow-null-check"
    ],
    "typeof-compare": true,
    "unnecessary-constructor": [
      true,
      {
        "check-super-calls": true
      }
    ],
    "use-isnan": true,
    "max-classes-per-file": [
      true,
      1,
      "exclude-class-expressions"
    ],
    "prefer-const": true,
    "arrow-return-shorthand": [
      true,
      "multiline"
    ],
    "class-name": true,
    "no-boolean-literal-compare": true,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "one-variable-per-declaration": [
      true,
      "ignore-for-loop"
    ],
    "space-within-parens": [
      true,
      1
    ],
    "arrow-parens": true,
    "eofline": true,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}",
        "check-strings": true,
        "check-regex": true
      }
    ],
    "newline-before-return": true,
    "new-parens": true,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-irregular-whitespace": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-operator",
      "check-typecast",
      "check-type-operator"
    ]
  }
}
